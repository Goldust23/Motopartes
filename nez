    <script>
        btn.onclick = function () {
            var buscar = document.getElementById("Buscar").value;
            if (buscar == "hola") {
                panel.innerHTML = "papayas";
            } else if (buscar == "perro") {
                panel.innerHTML = "perra";
            } else {
                panel.innerHTML = "Not Found";
            }

        }
    </script>








